// ====================
void setup() {
  display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS);
  display.clearDisplay();
}

void loop() {
  // 5 framelik animasyonu sonsuz döngüyle oynat
  for (int i = 0; i < frameCount; i++) {
    display.clearDisplay();
    // Görüntüleri ortalamak için x=32, y=-32 (dikey mod)
    display.drawBitmap(0, -0, frames[i], 64, 128, WHITE);
    display.setRotation(1);
    display.display();
    delay(150); // kareler arası hız (ms)
  }
}
